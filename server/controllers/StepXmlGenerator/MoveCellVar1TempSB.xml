<task>
	<preload>
		<comps>
			<comp id="1" name="SIMS.Components.Excel.Ribbon" cssclass="SIMS_Ribbon_Excel" type="default"/>
			<comp id="2" name="SIMS.Components.Excel.JSONGrid" cssclass="" type="default"/>
			<comp id="3" name="SIMS.Components.Common.TaskBar" cssclass="SIMS_TaskBar" type="default"/>
		</comps>
	</preload>
	<states>
		<state id="1" desc="Excel document opened">
			<comps>
				<comp id="1" mode="new">
					<sizeandpos>
						<attr name="left" value="0"/>
						<attr name="top" value="0"/>
						<attr name="width" value="*"/>
						<attr name="height" value="150"/>
						<attr name="aaa" userDefined="true"/>
						<attr name="bbb" userDefined="true"/>
					</sizeandpos>
					
					<initialattrs>
						<attr name="APP" value="excel"/>
						<attr name="RIBBON_PATH" value="app/comps2016/Common/Ribbon/excel-ribbon.xml"/>
						<attr name="DOCUMENT_NAME" userDefined="true"/>
						<attr name="SEL_TAB" value="Formulas"/>
					</initialattrs>
					<initialattributesets>
						<attributeset name="set_1">
							<attr name="APP" value="excel"/>
							<uattr name="APP1" userDefined="true"/>
						</attributeset>
						<attributeset name="set_2">
							<attr name="APP" value="excel"/>
						</attributeset>
					</initialattributesets>
					<finalattrs>
						<attr name="APP" value="excel"/>
						<attr name="Final_APP1" userDefined="true"/>
					</finalattrs>
					<finalattributesets>
						<attributeset name="set_1">
							<attr name="APP" value="excel"/>
							<attr name="APP1" userDefined="true"/>
						</attributeset>
						<attributeset name="set1_" multiple-occurence="true" based-on="attr" dependency-attr='["SELECTED_CELLS","COPY_HIGHLIGHT_CELLS"]'>
							<attr name="SELECTED_CELLS" userDefined="true"/>
							<attr name="COPY_HIGHLIGHT_CELLS" userDefined="true"/>
							<attr name="XYZ" value="true"/>
						</attributeset>
					</finalattributesets>
					<events>
						<event id="142" desc="Cut:ClipBoard" >
							<validate followup="3" target-attribute-set="cut1">
								<comp id="2" validation-set="set1"/>
							</validate>							
							<validate followup="2" operator="any">
								<comp id="3" validation-set="" multiple-occurence="true" based-on="finalAttrSet" dependency-set="set1asds_"/>
							</validate>
						</event>
					</events>
				</comp>
				<comp id="2" mode="new">
					<sizeandpos>
						<attr name="left" value="0"/>
						<attr name="top" value="147"/>
						<attr name="width" value="*"/>
					</sizeandpos>
					<initialattrs>
						<attr name="INIT_DOC_JSON" userDefined="true"/>
						<attr name="IMAGE_JSON" userDefined="true"/>
					</initialattrs>
					<finalattributesets>
						<attributeset name="set1">
							<attr name="SELECTED_CELLS" userDefined="true"/>
						</attributeset>
						<attributeset name="set3">
							<attr name="SEL_DRAG_CELL" userDefined="true"/>
						</attributeset>
					</finalattributesets>
					<events>
						<event id="89" desc="Selection correctly dragged">
							<validate followup="2">
								<comp id="2" validation-set="set3"/>
							</validate>
						</event>
						<event id="35" desc="Cut SEL:Shortcut">
							<validate followup="3" target-attribute-set="cut1">
								<comp id="2" validation-set="set1"/>
							</validate>
						</event>
						<event id="3555" desc="Dummy">
							<validate followup="2" operator="any">
								<comp id="1" validation-set="" multiple-occurence="true" based-on="finalAttrSet" dependency-set="set1_"/>
							</validate>
						</event>
					</events>
				</comp>
				<comp id="3" mode="new">
					<sizeandpos>
						<attr name="left" value="0"/>
						<attr name="bottom" value="0"/>
						<attr name="width" value="*"/>
						<attr name="height" value="31"/>
					</sizeandpos>
					<initialattrs>
						<attr name="APPLICATION" value="excel"/>
						<attr name="APP_TOOLTIP_IMAGE" userDefined="true"/>
						<attr name="OPENWORDFILE" userDefined="true"/>
					</initialattrs>
					<finalattributesets>
						<attributeset name="set_2">
							<attr name="APP" value="excel"/>
							<attr name="APP1" userDefined="true"/>
						</attributeset>
						<attributeset name="set2_" multiple-occurence="true" based-on="attr" dependency-attr='["SELECTED_CELLS","COPY_HIGHLIGHT_CELLS"]'>
							<attr name="SELECTED_CELLS" userDefined="true"/>
							<attr name="COPY_HIGHLIGHT_CELLS" userDefined="true"/>
						</attributeset>
					</finalattributesets>
				</comp>
			</comps>
		</state>
	</states>
</task>

