{
    "views": [
        {
            "id": "1",
            "label": "Initial Document Settings",
            "itemType": "root",
            "desc": {},
            "items": [
                {
                    "id": "1.1",
                    "label": "Ribbon",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.PPT.Ribbon",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "1.1.1",
                            "label": "Document Title",
                            "desc": {
                                "basic": "Name of the document to be displayed in Ribbon.",
                                "detailed": "Document Name which is shown in the central top area in the Ribbon is to be mentioned here."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.1.documentTitle}}"
                        }
                    ]
                },
                {
                    "id": "1.2",
                    "label": "Slide View",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.PPT.SlideView",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "1.2.1",
                            "label": "Slide View Data",
                            "desc": {
                                "basic": "Upload a XML file which is used to configure the Slide View Area in SIMS.",
                                "detailed": "A XML file, which will be used to configure the Slide View Area in SIMS, is required to be uploaded here."
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "emitEvents": [
                                "SLIDE_VIEW_DATA_UPDATED"
                            ],
                            "rendererProperties": {
                                "dataType": "XML",
                                "placeHolder": "Drop XML file here to upload"
                            },
                            "mandatory": true,
                            "val": "{{views.1.slideViewData}}"
                        },
                        {
                            "id": "1.2.2",
                            "label": "Slide Images",
                            "desc": {
                                "basic": "Upload the images which are used to render slides in SIMS.",
                                "detailed": "This section requires the images of the slides which we can get by saving presentation in .jpg format."
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "rendererProperties": {
                                "placeholder": "Drop slide Image",
                                "dataType": "img",
                                "multipleFiles": true
                            },
                            "mandatory": true,
                            "val": "{{views.1.slideImages}}"
                        },
                        {
                            "id": "1.2.3",
                            "label": "Inital Selected Slide",
                            "desc": {
                                "basic": "Index of the slide which is selected initially.",
                                "detailed": "Index of the slide which is selected initially."
                            },
                            "subscribeEvents": [
                                {
                                    "eventId": "SLIDE_VIEW_DATA_UPDATED",
                                    "modelReference": "{{views.1.initialSelectedSlide}}",
                                    "rule": "configureSlidesDropdown"
                                }
                            ],
                            "itemRenderer": "Dropdown",
                            "rendererProperties": {
                                "dynamicMode": true,
                                "itemList": "{{views.1.initialSelectedSlide.options}}"
                            },
                            "itemType": "leaf",
                            "mandatory": true,
                            "val": "{{views.1.initialSelectedSlide.selectedSlide}}"
                        }
                    ]
                },
                {
                    "id": "1.3",
                    "label": "Taskbar",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.Common.TaskBar",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "1.3.1",
                            "label": "Taskbar Preview Image",
                            "desc": {
                                "basic": "Upload taskbar preview image.",
                                "detailed": "When a user hovers mouse over the application iconm show in the task bar an image gets opened in the tool tip. That image has to be uploaded here."
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "rendererProperties": {
                                "dataType": "img",
                                "placeHolder": "Drop image here to upload"
                            },
                            "mandatory": false,
                            "val": "{{views.1.taskbarPreviewImage}}"
                        }
                    ]
                }
            ]
        },
        {
            "id": "2",
            "label": "Skill Specific Inputs",
            "itemType": "root",
            "desc": {},
            "items": [
                {
                    "id": "2.1",
                    "label": "Slide View",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.PPT.SlideView",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "2.1.1",
                            "label": "Slide in Action",
                            "desc": {
                                "basic": "Index of the slide in which the audio is to be inserted.",
                                "detailed": "Index of the slide on which the audio has to be inserted is to be mentioned here."
                            },
                            "subscribeEvents": [
                                {
                                    "eventId": "SLIDE_VIEW_DATA_UPDATED",
                                    "modelReference": "{{views.2.slideInAction}}",
                                    "rule": "configureSlidesDropdown"
                                }
                            ],
                            "itemRenderer": "Dropdown",
                            "rendererProperties": {
                                "dynamicMode": true,
                                "itemList": "{{views.2.slideInAction.options}}"
                            },
                            "itemType": "leaf",
                            "mandatory": true,
                            "val": "{{views.2.slideInAction.selectedSlide}}"
                        }
                    ]
                },
                {
                    "id": "2.2",
                    "label": "Image Place Holder",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.PPT.ImagePlaceHolder",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "2.2.1",
                            "label": "Mode of Use",
                            "desc": {
                                "basic": "Mode in which component will be used.",
                                "detailed": ""
                            },
                            "itemRenderer": "Radio",
                            "rendererProperties": {
                                "itemList": [
                                {
                                    "value":"1",
                                    "label":"Use Image Place Holder component."
                                },
                                {
                                    "value":"2",
                                    "label":"Use Image with Caption Place Holder component."
                                }                  
                                ]
                            },
                            "itemType": "leaf",
                            "emitEvents": [
                                "IMAGE_PH_COMP_UPDATE"
                            ],
                            "mandatory": true,
                            "val": "{{views.2.compName}}"
                        },
                        {
                            "id": "2.2.2",
                            "label": "Image to be cropped",
                            "desc": {
                                "basic": "Upload image which will be cropped.",
                                "detailed": ""
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "rendererProperties": {
                                "dataType": "img",
                                "placeHolder": "Drop image here to upload"
                            },
                            "mandatory": true,
                            "val": "{{views.2.cropImage}}"
                        },
                        {
                            "id": "2.2.3",
                            "label": "Image Configuration",
                            "desc": {
                                "basic": "Upload an XML having configuration for Image",
                                "detailed": ""
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "rendererProperties": {
                                "dataType": "xml",
                                "placeHolder": "Drop xml here to upload"
                            },
                            "mandatory": true,
                            "val": "{{views.2.imageConfig}}"
                        },
                        {
                            "id": "2.2.4",
                            "label": "Image Border Configuration",
                            "desc": {
                                "basic": "",
                                "detailed": ""
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "subscribeEvents": [
                                {
                                    "eventId": "IMAGE_PH_COMP_UPDATE",
                                    "modelReference": "{{views.2.imgBorderConfig}}",
                                    "rule": "feSwitchComponent"
                                }
                            ],
                            "val": "{{views.2.imgBorderConfig}}"
                        },
                        {
                            "id": "2.2.5",
                            "label": "Caption Border Configuration",
                            "desc": {
                                "basic": "",
                                "detailed": ""
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "subscribeEvents": [
                                {
                                    "eventId": "IMAGE_PH_COMP_UPDATE",
                                    "modelReference": "{{views.2.imgCaptionBorderConfig}}",
                                    "rule": "feSwitchComponent"
                                }
                            ],
                            "val": "{{views.2.imgCaptionBorderConfig}}"
                        }
                    ]
                },
                {
                    "id": "2.2",
                    "label": "Crop View Settings",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.PPT.ImagePlaceHolder",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": false
                    },
                    "items": [
                        {
                            "id": "2.2.1",
                            "label": "Crop View Image Configuration",
                            "desc": {
                                "basic": "A JSON having the configuration for the border that appears on the image while cropping.",
                                "detailed": "A JSON having the configuration for the border that appears on the image while cropping."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.2.cropViewImageConfiguration}}"
                        },
                        {
                            "id": "2.2.2",
                            "label": "Cropped View Image Configuration",
                            "desc": {
                                "basic": "A JSON having the configuration for the cropped image position and dimensions.",
                                "detailed": "A JSON having the configuration for the cropped image position and dimensions."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.2.croppedViewImageConfiguration}}"
                        },
                        {
                            "id": "2.2.3",
                            "label": "Crop View Image Border Configuration",
                            "desc": {
                                "basic": "A JSON having the configuration for the image position and dimensions.",
                                "detailed": "A JSON having the configuration for the image position and dimensions."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "subscribeEvents": [
                                {
                                    "eventId": "IMAGE_PH_COMP_UPDATE",
                                    "modelReference": "{{views.2.cropViewImageBorderConfiguration}}",
                                    "rule": "feSwitchComponent"
                                }
                            ],
                            "val": "{{views.2.cropViewImageBorderConfiguration}}"
                        }
                    ]
                }
            ]
        },
        {
            "id": "3",
            "label": "Final Document Settings",
            "itemType": "root",
            "desc": {},
            "items": [
                {
                    "id": "3.1",
                    "label": "Slide View",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.PPT.SlideView",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": false
                    },
                    "items": [
                        {
                            "id": "3.1.1",
                            "label": "Updated Slide Image",
                            "desc": {
                                "basic": "Upload the updated image of the slide.",
                                "detailed": "Upload the updated image of the slide."
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "rendererProperties": {
                                "placeHolder": "Drop Updated Slide Image",
                                "dataType": "img"
                            },
                            "mandatory": true,
                            "val": "{{views.3.updatedSlideImage}}"
                        }
                    ]
                },
                {
                    "id": "3.2",
                    "label": "Cropped Image Placeholder",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.PPT.ImagePlaceHolder",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": false
                    },
                    "items": [
                        {
                            "id": "3.2.1",
                            "label": "Cropped Image",
                            "desc": {
                                "basic": "Upload the cropped image file.",
                                "detailed": "Upload the cropped image file."
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "rendererProperties": {
                                "placeHolder": "Drop Cropped Image",
                                "dataType": "img"
                            },
                            "mandatory": true,
                            "val": "{{views.3.croppedImage}}"
                        },
                        {
                            "id": "3.2.2",
                            "label": "Cropped Image Configuration",
                            "desc": {
                                "basic": "A JSON having the configuration for the cropped image position and dimensions.",
                                "detailed": "A JSON having the configuration for the cropped image position and dimensions."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.3.croppedImageConfiguration}}"
                        }
                    ]
                }
            ]
        }
    ]
}