{
  "views": [
    {
      "id": "1",
      "label": "Initial Document Settings",
      "itemType": "root",
      "desc": {},
      "items": [
        {
          "id": "1.1",
          "label": "Ribbon",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Excel.Ribbon",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.1.1",
              "label": "Document Title",
              "desc": {
                "basic": "Name of the document to be displayed in Ribbon.",
                "detailed": "Document Name which is shown in the central top area in the Ribbon is to be mentioned here."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": false,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.1.documentTitle}}"
            }
          ]
        },
        {
          "id": "1.2",
          "label": "Workbook",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Excel.JSONGrid",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.2.1",
              "label": "Document Data",
              "desc": {
                "basic": "Upload a JSON file which is used to configure the Excel workbook in SIMS.",
                "detailed": "A JSON file, which will be used to configure the Excel workbook in SIMS, is required to be uploaded here. This file can be generated from here(Hyperlink pointing to the JSON creation tool spec)."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "JSON",
                "placeHolder": "Drop JSON file here to upload"
              },
              "dependants": [
                {
                  "modelReference": "{{views.1.sheets.value}}",
                  "rule": "getSheetNameAndSheetCountFromInitDocJSON"
                },
                {
                  "modelReference": "{{views.2.sheetInAction.options.value}}",
                  "rule": "addSheetNamesToDropdown"
                }
              ],
              "mandatory": true,
              "val": "{{views.1.documentData}}"
            },
            {
              "id": "1.2.2",
              "label": "Worksheet Images",
              "desc": {
                "basic": "Upload the images which are used to render worksheets in SIMS.",
                "detailed": "This section requires 4 separate images to be uploaded. These images will be used to render a worksheet in SIMS. Details on how to get these images  can be accessed from here (Hyperlink pointing to the Image creation process spec).Images for any number of sheets can be uploaded by pressing the (+) button shown in the right."
              },
              "itemRenderer": "TabGroup",
              "itemType": "child",
              "disabled": true,
              "rendererProperties": {
                "layout": "horizontal",
                "dynamicMode": true,
                "itemListRef": "{{views.1.sheets}}",
                "style": {
                  "max-width": "450px"
                }
              },
              "items": [
                {
                  "id": "1.2.2.1",
                  "itemRenderer": "TabPage",
                  "itemType": "",
                  "rendererProperties": {
                    "dynamicMode": true
                  },
                  "items": [
                    {
                      "id": "GridImage",
                      "itemRenderer": "Dropzone",
                      "itemType": "leaf",
                      "rendererProperties": {
                        "placeHolder": "Drop Grid Image",
                        "dynamicMode": true,
                        "dataType": "img"
                      },
                      "pos": "x,y",
                      "dim": {
                        "height": "200px",
                        "width": "200px"
                      },
                      "relVal": "gridImage"
                    },
                    {
                      "id": "RowImage",
                      "itemRenderer": "Dropzone",
                      "itemType": "leaf",
                      "rendererProperties": {
                        "placeHolder": "Drop Row Image",
                        "dynamicMode": true,
                        "dataType": "img"
                      },
                      "pos": "x,y",
                      "dim": {
                        "height": "200px",
                        "width": "200px"
                      },
                      "relVal": "rowImage"
                    },
                    {
                      "id": "ColumnImage",
                      "itemRenderer": "Dropzone",
                      "itemType": "leaf",
                      "rendererProperties": {
                        "placeHolder": "Drop Column Image",
                        "dynamicMode": true,
                        "dataType": "img"
                      },
                      "pos": "x,y",
                      "dim": {
                        "height": "200px",
                        "width": "200px"
                      },
                      "relVal": "columnImage"
                    },
                    {
                      "id": "CellImage",
                      "itemRenderer": "Dropzone",
                      "itemType": "leaf",
                      "rendererProperties": {
                        "placeHolder": "Drop Cell Image",
                        "dynamicMode": true,
                        "dataType": "img"
                      },
                      "pos": "x,y",
                      "dim": {
                        "height": "200px",
                        "width": "200px"
                      },
                      "relVal": "cellImage"
                    }
                  ]
                }
              ]
            },
            {
              "id": "1.2.3",
              "label": "Pre-filled Selection Cell Range",
              "itemRenderer": "Panel",
              "itemType": "child",
              "tags": [],
              "rendererProperties": {
                "collapsable": true,
                "optionalItems": false
              },
              "items": [
                {
                  "id": "1.2.3.1",
                  "label": "Radio Button Label",
                  "desc": {
                    "basic": "Not Available",
                    "detailed": "Not Available"
                  },
                  "itemRenderer": "Radio",
                  "items" : [
                    {
                      "label": "Yes",
                      "value": true
                    },
                    {
                      "label": "No",
                      "value": false
                    }
                  ],
                  "itemType": "leaf",
                  "mandatory": true,
                  "dependants": [
                    {
                      "modelReference": "{{views.1.cellRange}}",
                      "rule": "show"
                    },
                    {
                      "modelReference": "{{views.1.cellValues}}",
                      "rule": "show"
                    },
                    {
                      "modelReference": "{{views.1.formulaResult}}",
                      "rule": "show"
                    }
                  ],
                  "val": "{{views.1.radios.selectedOption}}"
                },
                {
                  "id": "1.2.3.2",
                  "label": "Cell Range",
                  "desc": {
                    "basic": "Not Available",
                    "detailed": "Not Available"
                  },
                  "itemRenderer": "TextBox",
                  "itemType": "leaf",
                  "mandatory": false,
                  "rendererProperties": {
                    "dataType": "string"
                  },
                  "val": "{{views.1.cellRange}}"
                },
                {
                  "id": "1.2.3.3",
                  "label": "Cell Values",
                  "desc": {
                    "basic": "Not Available",
                    "detailed": "Not Available"
                  },
                  "itemRenderer": "TextBox",
                  "itemType": "leaf",
                  "mandatory": false,
                  "rendererProperties": {
                    "dataType": "string"
                  },
                  "val": "{{views.1.cellValues}}"
                },
                {
                  "id": "1.2.3.4",
                  "label": "Formula Result",
                  "desc": {
                    "basic": "Not Available",
                    "detailed": "Not Available"
                  },
                  "itemRenderer": "TextBox",
                  "itemType": "leaf",
                  "mandatory": false,
                  "rendererProperties": {
                    "dataType": "string"
                  },
                  "val": "{{views.1.formulaResult}}"
                }
              ]
            }
          ]
        },
        {
          "id": "1.3",
          "label": "Taskbar",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Common.TaskBar",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.3.1",
              "label": "Taskbar Preview Image",
              "desc": {
                "basic": "Upload taskbar preview image.",
                "detailed": "When a user hovers mouse over the application iconm show in the task bar an image gets opened in the tool tip. That image has to be uploaded here."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "img",
                "placeHolder": "Drop image here to upload"
              },
              "mandatory": false,
              "val": "{{views.1.taskbarPreviewImage}}"
            }
          ]
        },
        {
          "id": "1.4",
          "label": "Add Component(s)",
          "itemRenderer": "Button",
          "itemType": "leaf",
          "rendererProperties": {
            "click": {},
            "disabled": true
          }
        }
      ]
    },
    {
      "id": "2",
      "label": "Skill Specific Inputs",
      "itemType": "root",
      "desc": {},
      "items": [
        {
          "id": "2.1",
          "label": "Workbook",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Excel.JSONGrid",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": false
          },
          "items": [
            {
              "id": "2.1.1",
              "label": "Sheet in Action",
              "desc": {
                "basic": "Name of the sheet in which the move cell content skill is to be performed.",
                "detailed": "Name of the sheet on which the move cell content skill has to be performed is to be mentioned here."
              },
              "itemRenderer": "Dropdown",
              "rendererProperties": {
                "itemListRef": "{{views.2.sheetInAction.options}}"
              },
              "itemType": "leaf",
              "mandatory": true,
              "dependants": [
                {
                  "modelReference": "{{views.3.sheets.value.0}}",
                  "rule": "updateSheetNameUsingDropdown"
                }
              ],
              "val": "{{views.2.sheetInAction.selectedOption}}"
            },
            {
              "id": "2.1.2",
              "label": "Formula Cell Range Reference",
              "desc": {
                "basic": "Cell Range from which the MAX has to be calculated is to be mentioned here in the format 'StartCell:EndCell'",
                "detailed": "Cell Range from which the MAX has to be calculated is to be mentioned here in the format 'StartCell:EndCell'"
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": true,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.2.formulaCellRangeReference}}"
            },
            {
              "id": "2.1.3",
              "label": "Cell containing formula",
              "desc": {
                "basic": "Cell Index in which the result has to be calculated is to be mentioned here.",
                "detailed": "Cell Index in which the result has to be calculated is to be mentioned here."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": true,
              "val": "{{views.2.cellContainingFormula}}"
            }
          ]
        }
      ]
    },
    {
      "id": "3",
      "label": "Final Document Settings",
      "itemType": "root",
      "desc": {},
      "items": [
        {
          "id": "3.1",
          "label": "Workbook",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Excel.JSONGrid",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": false
          },
          "items": [
            {
              "id": "3.1.1",
              "label": "Grid Image",
              "desc": {
                "basic": "Grid image after the 'skill_name' operation has been performed is to be uploadde here.",
                "detailed": "A JSON file is to be provided which will be utilised to display the necessary changes in the worksheet after performing the skill. This file can be generated from here(Hyperlink pointing to the JSON creation tool spec)."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "JSON",
                "placeHolder": "Drop JSON file here to upload"
              },
              "mandatory": true,
              "val": "{{views.3.gridImage}}"
            }
          ]
        }
      ]
    }
  ]
}