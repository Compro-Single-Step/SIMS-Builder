{
  "views": [
    {
      "id": "1",
      "label": "Initial Document Settings",
      "itemType": "root",
      "desc": {},
      "items": [
        { 
          "id": "1.1",
          "label": "Ribbon",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Word.Ribbon",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.1.1",
              "label": "Document Title",
              "desc": {
                "basic": "Name of the document to be displayed in Ribbon.",
                "detailed": "Document name which is shown in the central top area of the Ribbon, is to be mentioned here."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.1.documentTitle}}"
            }
          ]
        },
        {
          "id": "1.2",
          "label": "Word Document",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Word.DocumentArea",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.2.1",
              "label": "Document Data",
              "desc": {
                "basic": "An HTML file, which will be used to configure the initial state of Word Document Area.",
                "detailed": "An HTML file, which will be used to configure the Word Document Area in SIMS, is required to be uploaded here. This file can be generated by exporting original word document as an HTML file."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "HTML",
                "placeholder": "Drop HTML file here to upload"
              },
              "emitEvents": [
                "INITIAL_HTML_FILE_UPLOADED"
              ],
              "mandatory": true,
              "val": "{{views.1.initialDocumentFile}}"
            },
            {
              "id": "1.2.2",
              "label": "Document Images",
              "desc": {
                "basic": "Images (if any) which are part of the HTML file used to configure the initial state of Word Document Area.",
                "detailed": "Image(s) which will be used to configure the Word Doc Area in SIMS, is/are required to be uploaded here. These images are generated while exporting the original word document as HTML file."
              },
              "subscribeEvents": [
                {
                  "eventId": "INITIAL_HTML_FILE_UPLOADED",
                  "modelReference": "{{views.1.initialDocumentImages}}",
                  "rule": "feHtmlFileUpload"
                }
              ],
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "img",
                "placeholder": "Drop Document Images",
                "multipleFiles": true
              },
              "mandatory": true,
              "val": "{{views.1.initialDocumentImages}}"
            },
            {
              "id": "1.2.3",
              "label": "Shape ID",
              "desc": {
                "basic": "ID of the Shape HTML element on which effect is to be applied. This should be same throughout all the resources used for this task. [e.g. shape1 - id attribute of shape tag in html - <shape id=\"shape1\">]",
                "detailed": "ID of the Shape HTML element on which effect is to be applied. This should be same throughout all the resources used for this task. [e.g. shape1 - id attribute of shape tag in html - <shape id=\"shape1\">]"
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": true,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.1.shapeId}}"
            }
          ]
        },
        {
          "id": "1.4",
          "label": "Taskbar",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Common.TaskBar",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.4.1",
              "label": "Taskbar Preview Image",
              "desc": {
                "basic": "Word Document preview image, which is seen on hovering over application icon in Task Bar.",
                "detailed": "When a user hovers mouse over the application icon in the task bar, an image is shown as a tool tip. That image has to be uploaded here."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "img",
                "placeholder": "Drop image here to upload"
              },
              "val": "{{views.1.taskbarPreviewImage}}"
            }
          ]
        }
      ]
    },
    {
      "id": "2",
      "label": "Skill Specific Settings",
      "itemType": "root",
      "desc": {},
      "items": [
        {
          "id": "2.1",
          "label": "Shape Style Data",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Word.DocumentArea",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "2.1.1",
              "label": "Shape Effect Category",
              "desc": {
                "basic": "Type of shape effect to be applied to text box",
                "detailed": "Type of shape effect to be applied to text box"
              },
              "itemRenderer": "Dropdown",
              "rendererProperties": {
                "dynamicMode": true,
                "itemList": "{{views.2.shapeEffectCategory.options}}",
                "updateModel": [
                  {
                    "modelReference": "{{views.2.shapeEffectCategory.options.value}}",
                    "rule": "feUpdateShapeEffectCategoryDropdown"
                  }
                ]
              },
              "itemType": "leaf",
              "emitEvents": [
                "SHAPE_EFFECT_CATEGORY_UPDATED"
              ],
              "val": "{{views.2.shapeEffectCategory.selectedOption}}"
            },
            {
              "id": "2.1.2",
              "label": "Shape Effect Name",
              "desc": {
                "basic": "Options available in the type of shape effect selected",
                "detailed": "Options available in the type of shape effect selected"
              },
              "itemRenderer": "Dropdown",
              "itemType": "leaf",
              "rendererProperties": {
                "dynamicMode": true,
                "itemList": "{{views.2.shapeEffectName.options}}"
              },
              "val": "{{views.2.shapeEffectName.selectedOption}}",
              "subscribeEvents": [
                {
                  "eventId": "SHAPE_EFFECT_CATEGORY_UPDATED",
                  "modelReference": "{{views.2.shapeEffectName.options.value}}",
                  "rule": "feUpdateShapeEffectNamesInDropdown"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "id": "3",
      "label": "Final Document Settings",
      "itemType": "root",
      "desc": {},
      "items": [
        {
          "id": "3.1",
          "label": "Word Document",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Word.DocumentArea",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "3.1.1",
              "label": "Document Data",
              "desc": {
                "basic": "An HTML file, which will be used to configure the final state of Word Doc Area.",
                "detailed": "An HTML file, which will be used to configure the final state of Word Doc Area in SIMS, is required to be uploaded here. This file can be generated by exporting the Word Document as HTML file after inserting required image."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "HTML",
                "placeholder": "Drop HTML file here to upload"
              },
              "emitEvents": [
                "FINAL_HTML_FILE_UPLOADED"
              ],
              "val": "{{views.3.finalDocumentFile}}"
            },
            {
              "id": "3.1.2",
              "label": "Document Images",
              "desc": {
                "basic": "Images (if any) which are part of the HTML file used to configure the final state of Word Document Area.",
                "detailed": "Image(s) which will be used to configure the Word Doc Area in SIMS, is/are required to be uploaded here. These images are generated while exporting the final word document as HTML file."
              },
              "subscribeEvents": [
                {
                  "eventId": "FINAL_HTML_FILE_UPLOADED",
                  "modelReference": "{{views.3.finalDocumentImages}}",
                  "rule": "feHtmlFileUpload"
                }
              ],
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "img",
                "placeholder": "Drop Document Images",
                "multipleFiles": true
              },
              "val": "{{views.3.finalDocumentImages}}"
            }
          ]
        }
      ]
    }
  ]
}