{
  "views": [
    {
      "id": "1",
      "label": "Initial Document Settings",
      "itemType": "root",
      "desc": {},
      "items": [
        {
          "id": "1.1",
          "label": "Ribbon",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Word.Ribbon",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.1.1",
              "label": "Document Title",
              "desc": {
                "basic": "Name of the document to be displayed in Ribbon.",
                "detailed": "Document name which is shown in the central top area of the Ribbon, is to be mentioned here."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": false,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.1.documentTitle}}"
            }
          ]
        },
        {
          "id": "1.2",
          "label": "Word Document",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Word.DocumentArea",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.2.1",
              "label": "Document Data",
              "desc": {
                "basic": "An HTML file, which will be used to configure the initial state of Word Document Area.",
                "detailed": "An HTML file, which will be used to configure the Word Document Area in SIMS, is required to be uploaded here. This file can be generated by exporting original word document as an HTML file."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "HTML",
                "placeHolder": "Drop HTML file here to upload"
              },
              "mandatory": true,
              "val": "{{views.1.initialDocumentFile}}"
            },
            {
              "id": "1.2.2",
              "label": "Document Images",
              "desc": {
                "basic": "Images which are part of the HTML file used to configure the initial state of Word Document Area.",
                "detailed": "Image(s) which will be used to configure the Word Doc Area in SIMS, is/are required to be uploaded here. These images are generated while exporting the original word document as HTML file."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "img",
                "placeHolder": "Drop Document Images"
              },
              "mandatory": true,
              "val": "{{views.1.initialDocumentImages.value.0}}"
            }
          ]
        },
        {
          "id": "1.3",
          "label": "Caret Position",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Word.DocumentArea",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.3.1",
              "label": "Element ID",
              "desc": {
                "basic": "ID of the HTML element on which caret needs to be placed at the launch.",
                "detailed": "In order to place cursor at a particular position, ID of the parent HTML element needs to be mentioned here."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": false,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.1.initialElement}}"
            },
            {
              "id": "1.3.2",
              "label": "Caret position",
              "desc": {
                "basic": "Caret position relative to the start of the parent HTML element.",
                "detailed": "Value of caret position at which the cursor needs to be placed at start of the task, relative to its parent HTML element."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": false,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.1.initialCursorPosition}}"
            }
          ]
        },
        {
          "id": "1.4",
          "label": "Taskbar",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Common.TaskBar",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "1.4.1",
              "label": "Taskbar Preview Image",
              "desc": {
                "basic": "Word Document preview image, which is seen on hovering over application icon in Task Bar.",
                "detailed": "When a user hovers mouse over the application icon in the task bar, an image is shown as a tool tip. That image has to be uploaded here."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "img",
                "placeHolder": "Drop image here to upload"
              },
              "mandatory": false,
              "val": "{{views.1.taskbarPreviewImage}}"
            }
          ]
        }
      ]
    },
    {
      "id": "2",
      "label": "Insert File Dialog Settings",
      "itemType": "root",
      "desc": {},
      "items": [
        {
          "id": "2.1",
          "label": "Insertion Point",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Word.DocumentArea",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "2.1.1",
              "label": "Element ID",
              "desc": {
                "basic": "ID of the HTML element on which caret needs to be placed, before inserting picture.",
                "detailed": "In order to validate placement of caret at a particular position, ID of the parent HTML element needs to be mentioned here."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": false,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.2.finalElement}}"
            },
            {
              "id": "2.1.2",
              "label": "Insertion Point",
              "desc": {
                "basic": "Caret position relative to the start of the parent HTML element.",
                "detailed": "Value of caret position at which the cursor needs to be placed before inserting picture, relative to its parent HTML element."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": false,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.2.finalCursorPosition}}"
            }
          ]
        },
        {
          "id": "2.2",
          "label": "Insert Picture",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Common.InsertFileDialog",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "2.2.1",
              "label": "Tree XML",
              "desc": {
                "basic": "Insert File Dialog tree XML",
                "detailed": "XML file which is used to create the Tree structure of left pane in the Save As Dialog Box."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "XML",
                "placeHolder": "Drop XML file here to upload"
              },
              "mandatory": true,
              "val": "{{views.2.treeXML}}"
            },
            {
              "id": "2.2.2",
              "label": "Tree Selected Item ID",
              "desc": {
                "basic": "ID of the item to be displayed selected in Dialog.",
                "detailed": "ID of the item in the left pane of the Insert File Dialog to be displayed as selected. This ID is present in the Tree XML."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": false,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.2.treeSelectedItemID}}"
            },
            {
              "id": "2.2.3",
              "label": "Image Thumbnail",
              "desc": {
                "basic": "Thumbnail of Image to be shown in Insert File Dialog.",
                "detailed": "Image file which is used to show the thumbnail in Insert File Dialog. User needs to click on the thumbnail to insert image in document."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "img",
                "placeHolder": "Drop Image file here to upload"
              },
              "mandatory": true,
              "val": "{{views.2.thumbnailImage}}"
            },
            {
              "id": "2.2.4",
              "label": "Image Name",
              "desc": {
                "basic": "Name of the image to be inserted.",
                "detailed": "Enter the name of the image which needs to be selected by the user in the dfialog. This name will be used for validation."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": false,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.2.imageFileName}}"
            }
          ]
        }
      ]
    },
    {
      "id": "3",
      "label": "Final Document Settings",
      "itemType": "root",
      "desc": {},
      "items": [
        {
          "id": "3.1",
          "label": "Word Document",
          "itemRenderer": "Panel",
          "itemType": "child",
          "tags": [
            {
              "compName": "SIMS.Components.Word.DocumentArea",
              "type": "default"
            }
          ],
          "rendererProperties": {
            "collapsable": true,
            "optionalItems": true
          },
          "items": [
            {
              "id": "3.1.1",
              "label": "Document Data",
              "desc": {
                "basic": "An HTML file, which will be used to configure the final state of Word Doc Area.",
                "detailed": "An HTML file, which will be used to configure the final state of Word Doc Area in SIMS, is required to be uploaded here. This file can be generated by exporting the Word Document as HTML file after inserting required image."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "HTML",
                "placeHolder": "Drop HTML file here to upload"
              },
              "mandatory": true,
              "val": "{{views.3.finalDocumentFile}}"
            },
            {
              "id": "3.1.2",
              "label": "Document Images",
              "desc": {
                "basic": "Images which are part of the HTML file used to configure the final state of Word Document Area.",
                "detailed": "Image(s) which will be used to configure the Word Doc Area in SIMS, is/are required to be uploaded here. These images are generated while exporting the final word document as HTML file."
              },
              "itemRenderer": "Dropzone",
              "itemType": "leaf",
              "rendererProperties": {
                "dataType": "img",
                "placeHolder": "Drop Document Images"
              },
              "mandatory": true,
              "val": "{{views.3.finalDocumentImages.value.0}}"
            },
            {
              "id": "3.1.3",
              "label": "Image ID",
              "desc": {
                "basic": "ID of the image to be shown as selected.",
                "detailed": "ID which is present on the img tag of the image in final HTML document needs to be entered. This image will be shown as selected after insertion in document."
              },
              "itemRenderer": "TextBox",
              "itemType": "leaf",
              "mandatory": false,
              "rendererProperties": {
                "dataType": "string"
              },
              "val": "{{views.3.selectedImageID}}"
            }
          ]
        }
      ]
    }
  ]
}