{
    "views": [
        {
            "id": "1",
            "label": "Initial Document Settings",
            "itemType": "root",
            "desc": {},
            "items": [
                {
                    "id": "1.1",
                    "label": "Ribbon",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.Word.Ribbon",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "1.1.1",
                            "label": "Document Title",
                            "desc": {
                                "basic": "Name of the document to be displayed in Ribbon.",
                                "detailed": "Document Name which is shown in the central top area in the Ribbon is to be mentioned here."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.1.documentTitle}}"
                        }
                    ]
                },
                {
                    "id": "1.2",
                    "label": "Document Area",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.Word.DocumentArea",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "1.2.1",
                            "label": "Document File",
                            "desc": {
                                "basic": "An HTML file, which will be used to configure the initial state of Word Doc Area.",
                                "detailed": "An HTML file, which will be used to configure the Word Doc Area in SIMS, is required to be uploaded here. This file can be generated by exporting Original Word Document as HTML file."
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "emitEvents": [
                                "INITIAL_DOCUMENT_FILE_UPDATED"
                            ],
                            "rendererProperties": {
                                "dataType": "img",
                                "placeHolder": "Drop HTML file here to upload"
                            },
                            "mandatory": true,
                            "val": "{{views.1.initialDocumentFile}}"
                        },
                        {
                            "id": "1.2.2",
                            "label": "File Images",
                            "desc": {
                                "basic": "Upload the images which are part of HTML file.",
                                "detailed": "Imgaes which are part of the Initial Document FIle are required to be uploaded here. These are obtained in a folder when Original Word Document is saved as HTML file."
                            },
                            "itemRenderer": "TabPage",
                            "itemType": "",
                            "rendererProperties": {
                                "dynamicMode": true
                            },
                            "subscribeEvents": [
                                {
                                    "eventId": "INITIAL_DOCUMENT_FILE_UPDATED",
                                    "modelReference": "{{views.1.initialDocumentImages.value}}",
                                    "rule": "getImagesInHTMLFile"
                                }
                            ],
                            "val": "{{views.1.initialDocumentImages}}",
                            "items": [
                                {
                                    "id": "1.2.2.1",
                                    "itemRenderer": "Dropzone",
                                    "itemType": "leaf",
                                    "rendererProperties": {
                                        "placeHolder": "Drop Document Image",
                                        "dynamicMode": true,
                                        "dataType": "img"
                                    },
                                    "pos": "x,y",
                                    "dim": {
                                        "height": "200px",
                                        "width": "200px"
                                    },
                                    "relVal": "documentImage"
                                }
                            ]
                        }
                    ]
                },
                {
                    "id": "1.3",
                    "label": "Cursor Positions",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.Word.DocumentArea",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "1.3.1",
                            "label": "Initial Parent element",
                            "desc": {
                                "basic": "Initial Parent element",
                                "detailed": "ID of the HTML element on which cursor needs to be placed at the launch."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.1.initialElement}}"
                        },
                        {
                            "id": "1.3.2",
                            "label": "Initial Cursor Position",
                            "desc": {
                                "basic": "Initial Cursor Position",
                                "detailed": "Value of Cursor Position at which the cursor is placed at start of the task relative to parent HTML element."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.1.initialCursorPosition}}"
                        },
                        {
                            "id": "1.3.3",
                            "label": "Final Parent element",
                            "desc": {
                                "basic": "Final Parent Element",
                                "detailed": "ID of the HTML element on which cursor needs to be placed."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.1.finalElement}}"
                        },
                        {
                            "id": "1.3.4",
                            "label": "Final Cursor Position",
                            "desc": {
                                "basic": "Final Cursor Position",
                                "detailed": "Value of Cursor Position at which the cursor is placed at end of the task, relative to parent HTML element."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.1.finalCursorPosition}}"
                        }
                    ]
                },
                {
                    "id": "1.4",
                    "label": "Taskbar",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.Common.TaskBar",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "1.4.1",
                            "label": "Taskbar Preview Image",
                            "desc": {
                                "basic": "Upload taskbar preview image.",
                                "detailed": "When a user hovers mouse over the application iconm show in the task bar an image gets opened in the tool tip. That image has to be uploaded here."
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "rendererProperties": {
                                "dataType": "img",
                                "placeHolder": "Drop image here to upload"
                            },
                            "mandatory": false,
                            "val": "{{views.1.taskbarPreviewImage}}"
                        }
                    ]
                }
            ]
        },
        {
            "id": "2",
            "label": "Insert File Dialog Settings",
            "itemType": "root",
            "desc": {},
            "items": [
                {
                    "id": "2.1",
                    "label": "Insert File Dialog",
                    "itemRenderer": "Panel",
                    "itemType": "child",
                    "tags": [
                        {
                            "compName": "SIMS.Components.Common.InsertFileDialog",
                            "type": "default"
                        }
                    ],
                    "rendererProperties": {
                        "collapsable": true,
                        "optionalItems": true
                    },
                    "items": [
                        {
                            "id": "2.1.1",
                            "label": "Tree XML",
                            "desc": {
                                "basic": "Insert File Dialog tree XML",
                                "detailed": "XML file which is used to create the Tree structure of left pane in the Save As Dialog Box."
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "rendererProperties": {
                                "dataType": "XML",
                                "placeHolder": "Drop XML file here to upload"
                            },
                            "mandatory": true,
                            "val": "{{views.2.treeXML}}"
                        },
                        {
                            "id": "2.1.2",
                            "label": "Tree Selected Item ID",
                            "desc": {
                                "basic": "ID of the item to be displayed selected in Dialog.",
                                "detailed": "ID of the item in the left pane of the Insert File Dialog to be displayed as selected. This ID is present in the Tree XML."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.2.treeSelectedItemID}}"
                        },
                        {
                            "id": "2.1.3",
                            "label": "Image Thumbnail",
                            "desc": {
                                "basic": "Thumbnail of Image to be shown in Insert File Dialog.",
                                "detailed": "Image file which is used to show the thumbnail in Insert File Dialog. User needs to click on the thumbnail to insert image in document."
                            },
                            "itemRenderer": "Dropzone",
                            "itemType": "leaf",
                            "rendererProperties": {
                                "dataType": "img",
                                "placeHolder": "Drop Image file here to upload"
                            },
                            "mandatory": true,
                            "val": "{{views.2.thumbnailImage}}"
                        },
                        {
                            "id": "2.1.4",
                            "label": "Image Name",
                            "desc": {
                                "basic": "Name of the image to be inserted.",
                                "detailed": "Enter the name of the image which needs to be selected by the user in the dfialog. This name will be used for validation."
                            },
                            "itemRenderer": "TextBox",
                            "itemType": "leaf",
                            "mandatory": false,
                            "rendererProperties": {
                                "dataType": "string"
                            },
                            "val": "{{views.2.imageFileName}}"
                        }
                    ]
                }
            ]
        },
        {
            "id": "3",
            "label": "Final Document Settings",
            "itemType": "root",
            "desc": {},
            "items": [
            {
                "id": "3.1",
                "label": "Document Area",
                "itemRenderer": "Panel",
                "itemType": "child",
                "tags": [
                    {
                        "compName": "SIMS.Components.Word.DocumentArea",
                        "type": "default"
                    }
                ],
                "rendererProperties": {
                    "collapsable": true,
                    "optionalItems": true
                },
                "items": [
                    {
                        "id": "3.1.1",
                        "label": "Document File",
                        "desc": {
                            "basic": "An HTML file, which will be used to configure the final state of Word Doc Area.",
                            "detailed": "An HTML file, which will be used to configure the final state of Word Doc Area in SIMS, is required to be uploaded here. This file can be generated by exporting the Word Document as HTML file after inserting required image."
                        },
                        "itemRenderer": "Dropzone",
                        "itemType": "leaf",
                        "emitEvents": [
                            "FINAL_DOCUMENT_FILE_UPDATED"
                        ],
                        "rendererProperties": {
                            "dataType": "img",
                            "placeHolder": "Drop HTML file here to upload"
                        },
                        "mandatory": true,
                        "val": "{{views.3.finalDocumentFile}}"
                    },
                    {
                        "id": "3.1.2",
                        "label": "File Images",
                        "desc": {
                            "basic": "Upload the images which are part of Final HTML file.",
                            "detailed": "Imgaes which are part of the Final Document File are required to be uploaded here. These are obtained in a folder when Final Word Document is saved as HTML file."
                        },
                        "itemRenderer": "TabPage",
                        "itemType": "",
                        "rendererProperties": {
                            "dynamicMode": true
                        },
                        "subscribeEvents": [
                            {
                                "eventId": "FINAL_DOCUMENT_FILE_UPDATED",
                                "modelReference": "{{views.3.finalDocumentImages}}",
                                "rule": "getImagesInHTMLFile"
                            }
                        ],
                        "val": "{{views.3.finalDocumentImages}}",
                        "items": [
                            {
                                "id": "3.1.2.1",
                                "itemRenderer": "Dropzone",
                                "itemType": "leaf",
                                "rendererProperties": {
                                    "placeHolder": "Drop Document Image",
                                    "dynamicMode": true,
                                    "dataType": "img"
                                },
                                "pos": "x,y",
                                "dim": {
                                    "height": "200px",
                                    "width": "200px"
                                },
                                "relVal": "documentImage"
                            }
                        ]
                    },
                    {
                        "id": "3.1.3",
                        "label": "Image ID",
                        "desc": {
                            "basic": "ID of the image to be shown as selected.",
                            "detailed": "ID which is present on the img tag of the image in final HTML document needs to be entered. This image will be shown as selected after insertion in document."
                        },
                        "itemRenderer": "TextBox",
                        "itemType": "leaf",
                        "mandatory": false,
                        "rendererProperties": {
                            "dataType": "string"
                        },
                        "val": "{{views.3.selectedImageID}}"
                    }
                ]
            }
        ]
        }
    ]
}