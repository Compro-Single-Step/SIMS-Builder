<task>
	<preload>
		<comps>
			<comp id="1" name="SIMS.Components.Excel.Ribbon" cssclass="SIMS_Ribbon_Excel" type="default"/>
			<comp id="2" name="SIMS.Components.Excel.JSONGrid" cssclass="" type="default"/>
			<comp id="3" name="SIMS.Components.Common.TaskBar" cssclass="SIMS_TaskBar" type="default"/>
		</comps>
	</preload>

	<states>
		<state id="1" desc="Excel document opened">
			<comps>
				<comp id="1" mode="new">
					<sizeandpos>
						<attr name="left" value="0"/>
						<attr name="top" value="0"/>
						<attr name="width" value="*"/>
						<attr name="height" value="150"/>
					</sizeandpos>
					<initialattrs>
						<attr name="APP" value="excel"/>
						<attr name="RIBBON_PATH" value="app/comps2016/Common/Ribbon/excel-ribbon.xml"/>
						<attr name="DOCUMENT_NAME" value="xls04_Garden - Excel"/>
					</initialattrs>
					<events>
						<event id="142" desc="Cut:ClipBoard">
							<validate followup="3" target-attribute-set="cut1">
								<comp id="2" validation-set="set1"/>
							</validate>
						</event>
					</events>
				</comp>
				<comp id="2" mode="new">
					<sizeandpos>
						<attr name="left" value="0"/>
						<attr name="top" value="147"/>
						<attr name="width" value="*"/>
					</sizeandpos>
					<initialattrs>
						<attr name="INIT_DOC_JSON" value="XMLs/TaskXmls2016/skl/xl/04/01.03.T1/Assets/State1/Res1.json"/>
						<attr name="IMAGE_JSON" value="{&quot;sheetImages&quot;:[{&quot;sheetNo&quot;:1,&quot;gridImg&quot;:&quot;Inventory_DataGrid.png&quot;,&quot;rowImg&quot;:&quot;Inventory_RowGrid.png&quot;,&quot;colImg&quot;:&quot;Inventory_CellImg.png&quot;,&quot;cellImg&quot;:&quot;Inventory_ColumnGrid.png&quot;}]}"/>
					</initialattrs>
					<finalattributesets>
						<attributeset name="set-1">
							<attr name="SELECTED_CELLS" value="G5:H6"/>
						</attributeset>
						<attributeset name="set-3">
							<attr name="SEL_DRAG_CELL" value="{&quot;sheetNo&quot;:1,&quot;startRange&quot;:&quot;G5:H6&quot;,&quot;endRange&quot;:&quot;D5:E6&quot;}"/>
						</attributeset>
					</finalattributesets>
					<events>
						<event id="89" desc="Selection correctly dragged">
							<validate followup="2">
								<comp id="2" validation-set="set3"/>
							</validate>
						</event>
						<event id="35" desc="Cut SEL:Shortcut">
							<validate followup="3" target-attribute-set="cut1">
								<comp id="2" validation-set="set1"/>
							</validate>
						</event>
					</events>
				</comp>
				<comp id="3" mode="new">
					<sizeandpos>
						<attr name="left" value="0"/>
						<attr name="bottom" value="0"/>
						<attr name="width" value="*"/>
						<attr name="height" value="31"/>
					</sizeandpos>
					<initialattrs>
						<attr name="APPLICATION" value="excel"/>
						<attr name="APP_TOOLTIP_IMAGE" value="XMLs/TaskXmls2016/skl/xl/04/01.03.T1/Assets/img/preview.jpg"/>
						<attr name="OPENWORDFILE" value="xls04_Garden - Excel"/>
					</initialattrs>
				</comp>
			</comps>
		</state>
        <state id="3" desc="Cut Action" txt="1">
            <comps>
                <comp id="1" mode="edit">
                    <initialattrs>
                        <attr name="RIBBON_SUB_XML" userDefined="true"/>
                        <attr name="SUB_XML_UPDATE" value="true"/>
                    </initialattrs>
                    <events>
                        <event id="148" desc="Paste Sel Clipboard">
                            <validate operator="any" followup="2"><comp id="2" validation-set="set1_1"/><comp id="2" validation-set="set1_2"/></validate>
                        </event>
                        <event id="51" desc="Insert : Insert Cut Cells">
                            <validate operator="any" followup="2"><comp id="2" validation-set="set1_1"/><comp id="2" validation-set="set1_2"/></validate>
                        </event>
                    </events>
                </comp>
                <comp id="2" mode="edit">
                    <initialattributesets>
                        <attributeset name="cut1" inherits-default="false">
                            <attr name="COPY_HIGHLIGHT_CELLS" value="{&quot;sheetNo&quot;:1,&quot;range&quot;:&quot;G5:H6&quot;}"/>
                        </attributeset>
                    </initialattributesets>
                    <finalattributesets>
                        <attributeset name="set1_1">
                            <attr name="SELECTED_CELLS" value="D5"/>
                            <attr name="COPY_HIGHLIGHT_CELLS" value="true"/>
                        </attributeset><attributeset name="set1_2">
                            <attr name="SELECTED_CELLS" value="D5:E5"/>
                            <attr name="COPY_HIGHLIGHT_CELLS" value="true"/>
                        </attributeset>
                    </finalattributesets>
                    <events>
                        <event id="36" desc="Paste SEL shortcut">
                            <validate followup="2" operator="any"><comp id="2" validation-set="set1_1"/><comp id="2" validation-set="set1_2"/></validate>
                        </event>
                        <event id="58" desc="Paste Special Dialog">
                            <validate followup="2" operator="any">
                                <comp id="2" validation-set="set1"/>
                                <comp id="2" validation-set="set2"/>
                                <comp id="2" validation-set="set3"/>
                                <comp id="2" validation-set="set4"/>
                            </validate>
                        </event>
                    </events>
                </comp>
                <comp id="3" mode="reference" ref-state="1"/>
            </comps>
        </state>
		<state id="2" desc="Text in D5" compstack="2" islast="true">
			<comps>
				<comp id="1" mode="edit">
					<sizeandpos>
						<attr name="left" value="0"/>
						<attr name="top" value="0"/>
						<attr name="width" value="*"/>
						<attr name="height" value="150"/>
					</sizeandpos>
				</comp>
				<comp id="2" mode="edit">
					<sizeandpos>
						<attr name="left" value="0"/>
						<attr name="top" value="147"/>
						<attr name="width" value="*"/>
					</sizeandpos>
					<initialattrs>
						<attr name="IMAGE_JSON" value="{&quot;sheetImages&quot;:[{&quot;sheetNo&quot;:1,&quot;gridImg&quot;:&quot;State2/Inventory_DataGrid.png&quot;,&quot;rowImg&quot;:&quot;State1/Inventory_RowGrid.png&quot;,&quot;colImg&quot;:&quot;State1/Inventory_CellImg.png&quot;,&quot;cellImg&quot;:&quot;State1/Inventory_ColumnGrid.png&quot;}]}"/>
						<attr name="SHEET_CELLS_DATA" value="{&quot;sheetNo&quot;:1,&quot;dataJSONPath&quot;:&quot;&quot;}"/>
						<attr name="SELECTED_CELLS" value="G5:H6"/>
						<attr name="COPY_HIGHLIGHT_CELLS" value="{&quot;sheetNo&quot;:&quot;1&quot;, &quot;range&quot;:&quot;&quot;}"/>
					</initialattrs>
				</comp>
				<comp id="3" mode="reference" ref-state="1"/>
			</comps>
		</state>	
	</states>
</task>